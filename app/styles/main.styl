@import 'jeet'
@import 'nib'

html, body
  height: 100%
  span(1)

#wrapper
  #header
    height: 60px
    span(1)
    position: fixed
    #logo
      col(1/3)
      font-size: 60px
      color: blue 
    #navigater
      col(1/2, offset: 1/2)
      a
        color: #0a64a4
        cursor: pointer
        :active
          color: black
  .page
    height: 600px
    background-color: #f1f1f1
    border-bottom: 2px solid


